#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>

//	0x10 000
//	b10000000000000000
//	0x2710
//	11110000 10010000 10000000 10000000

//	0xxxxxxx								-> 7bits
//	110xxxxx 10xxxxxx						-> 11bits
//	1110xxxx 10xxxxxx 10xxxxxx				-> 16bits
//	11110xxx 10xxxxxx 10xxxxxx 10xxxxxx		-> 21bits

int	main(void)
{
    long long c;
//	0100 0001;
    c = 0x41;
    write(1, &c, 1);

	return (0);
}
